{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./api"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "path": "ink-docstrap",
    "theme": "journal",
    "navType": "vertical",
    "inverseNav": true,
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "outputSourceFiles": true,
    "outputSourcePath": true
  }
}